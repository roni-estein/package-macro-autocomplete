{
    "name": "roni-estein/package-macro-autocomplete",
    "description": "A helper package that allows barryvdh/laravel-ide-helper find and index macros stored in packages ",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Roni Estein",
            "email": "roni.estein@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "PackageMacroAutocomplete\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PackageMacroAutocomplete\\ServiceProvider"
            ]
        }
    }
}